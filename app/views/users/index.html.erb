<% unless current_user.nil? %>
  <h1> You are <%= link_to current_user.nickname, current_user %> </h1>
<% end %>

<ul>
<% @users.each do |user| %>
  <li>
  <%= user.id %>. <%= link_to user.nickname, user %>
  <%= link_to 'edit', edit_user_path(user) %>
  <%= delete_link user, "Delete type #{user.nickname}" %>
<% end %>
</ul>

<%= link_to "Sign up", new_registration_path(:user), class: 'button' %>
